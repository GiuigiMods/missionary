<append xpath="/buffs/buff[@name='buffStatusCheck02']">
	<effect_group>
		<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffBlessedSetBonus">
			<requirement name="ArmorGroupCount" group_name="groupBlessed" operation="LTE" value="3"/>
			<requirement name="HasBuff" buff="buffBlessedSetBonus"/>
		</triggered_effect>
		<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="buffBlessedSetBonus">
			<requirement name="ArmorGroupCount" group_name="groupBlessed" operation="Equals" value="4"/>
			<requirement name="!HasBuff" buff="buffBlessedSetBonus"/>
		</triggered_effect>

		<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffBlessedFullSetBonusCheck">
			<requirement name="ArmorGroupCount" group_name="groupBlessed" operation="LTE" value="3"/>
			<requirement name="HasBuff" buff="buffBlessedFullSetBonusCheck"/>
		</triggered_effect>
		<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffBlessedFullSetBonusCheck">
			<requirement name="ArmorGroupLowestQuality" group_name="groupBlessed" operation="LTE" value="5"/>
			<requirement name="HasBuff" buff="buffBlessedFullSetBonusCheck"/>
		</triggered_effect>
		<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="buffBlessedFullSetBonusCheck">
			<requirement name="ArmorGroupCount" group_name="groupBlessed" operation="Equals" value="4"/>
			<requirement name="ArmorGroupLowestQuality" group_name="groupBlessed" operation="Equals" value="6"/>
			<requirement name="!HasBuff" buff="buffBlessedFullSetBonusCheck"/>
		</triggered_effect>
	</effect_group>
</append>

<append xpath="/buffs/buff[@name='buffOnAnyCrit']">
	<effect_group>
			<requirement name="CVarCompare" cvar="noTeethNoInfection" operation="LT" value="1"/>
			<requirement name="!HasBuff" buff="buffBlessedFullSetBonusCheck"/>
				<passive_effect name="BuffResistance" operation="base_add" value="-.1" tags="buffInfectionCatch"/>
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" buff="buffInfectionCatch"/>
		</effect_group>
</append>

<prepend xpath="/buffs/buff[not(starts-with(@name,'buffBlessedSetBonus')) and not(starts-with(@name,'buffPilgrimagingMissionaryG')) and not(starts-with(@name,'buff44FlagBlessingMissionaryG')) and not(starts-with(@name,'buffStatusCheck01')) and not(starts-with(@name,'buffStatusCheck02')) and not(starts-with(@name,'buffBlessedFullSetBonusCheck')) and not(starts-with(@name,'buffInfectedMissionaryG')) and not(tags)]">
	<tags value="new_journey"/>
</prepend>

<csv xpath="/buffs/buff/tags/@value" op="Add">new_journey</csv>

